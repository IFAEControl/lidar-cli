<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
<Appenders>
    <Console name="Console" target="SYSTEM_ERR">
        <PatternLayout pattern="%d{HH:mm:ss.SSS} %-5level %logger{4} - %msg%n"/>
    </Console>
</Appenders>
<Loggers>
    <!--
    <Logger name="cat.ifae.cta.container.lidar.microInterface.control.MicroControlClient" level="trace" additivity="false">
        <AppenderRef ref="Console"/>
    </Logger>
    -->

    <Root level="info">
        <AppenderRef ref="Console"/>
    </Root>
</Loggers>
</Configuration>
